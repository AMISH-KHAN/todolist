<%- include("header");-%>
<body>
    <div class="box" id="heading">
        

            <h1 > <%= listtitle %></h1>
        
    </div>
    <div class="box">
       
        <%addeditem.forEach((item)=>{%>
            <div class="item">
            <input type="checkbox">
                <p><%= item.Name %></p>
            </div>
            <%})%>

        <form action="/" method="post">
            <input type="text" name="item" id="item" placeholder="add item" autocomplete="off">
            <button type="submit" name="submit" value=<%= listtitle %>>+</button>
        </form>
    </div>
</body>
<%- include("footer");-%>
</html>